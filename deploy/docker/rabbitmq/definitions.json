{
  "rabbit_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password": "admin",
      "tags": ["administrator"]
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "middleware.direct",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "middleware.topic",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "middleware.dlx",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "inbound.processor",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-dead-letter-exchange": "middleware.dlx",
        "x-dead-letter-routing-key": "inbound.processor.dlq",
        "x-max-length": 10000,
        "x-max-length-bytes": 104857600,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "inbound.processor.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 1000
      }
    },
    {
      "name": "outbound.dispatcher",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 86400000,
        "x-dead-letter-exchange": "middleware.dlx",
        "x-dead-letter-routing-key": "outbound.dispatcher.dlq",
        "x-max-length": 10000,
        "x-max-length-bytes": 104857600,
        "x-overflow": "reject-publish"
      }
    },
    {
      "name": "outbound.dispatcher.dlq",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 1000
      }
    }
  ],
  "bindings": [
    {
      "source": "middleware.direct",
      "vhost": "/",
      "destination": "inbound.processor",
      "destination_type": "queue",
      "routing_key": "inbound.processor",
      "arguments": {}
    },
    {
      "source": "middleware.dlx",
      "vhost": "/",
      "destination": "inbound.processor.dlq",
      "destination_type": "queue",
      "routing_key": "inbound.processor.dlq",
      "arguments": {}
    },
    {
      "source": "middleware.direct",
      "vhost": "/",
      "destination": "outbound.dispatcher",
      "destination_type": "queue",
      "routing_key": "outbound.dispatcher",
      "arguments": {}
    },
    {
      "source": "middleware.dlx",
      "vhost": "/",
      "destination": "outbound.dispatcher.dlq",
      "destination_type": "queue",
      "routing_key": "outbound.dispatcher.dlq",
      "arguments": {}
    }
  ]
} 